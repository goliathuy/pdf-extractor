{
  "processing": {
    "white_text_threshold": 15000000,
    "min_image_size": 10,
    "fuzzy_match_threshold": 0.6,
    "fuzzy_match_confidence_threshold": 0.7,
    "default_equal_parts": 4,
    "max_pages_per_process": 1000,
    "enable_memory_monitoring": true
  },
  "output": {
    "create_timestamp_dirs": true,
    "base_output_dir": "processed_data",
    "text_filename": "extracted_text.txt",
    "json_filename": "extracted_content.json",
    "section_info_filename": "section_info.json",
    "processing_summary_filename": "processing_summary.json",
    "images_dirname": "extracted_images"
  },
  "sections": {
    "Message From Founders": {"start": 3, "end": 4},
    "General Information": {"start": 5, "end": 31},
    "Sales": {"start": 32, "end": 78},
    "Business Location A": {"start": 79, "end": 92},
    "Business Location B": {"start": 93, "end": 96},
    "Miscellaneous": {"start": 97, "end": 112}
  },
  "validation": {
    "check_page_overlaps": true,
    "validate_page_ranges": true,
    "max_pdf_size_mb": 100,
    "warn_on_overlap": true,
    "validate_section_confidence": true,
    "min_section_confidence": 0.5
  },
  "logging": {
    "level": "INFO",
    "enable_progress_bars": true,
    "log_memory_usage": true,
    "detailed_error_reporting": true
  },
  "performance": {
    "batch_size": 10,
    "parallel_processing": false,
    "memory_limit_mb": 512,
    "cleanup_intermediate_files": false
  }
}
